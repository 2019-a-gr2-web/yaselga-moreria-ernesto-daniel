<style>
    table,tr,td,th{
        border: solid black 1px;
    }
</style>

<a href="/api/traguito/crear">
    <button>Crear</button>
</a>


<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <% arregloTragos.forEach((trago)=>{ %>
    <tr>
        <td> <%=trago.nombre%> </td>
        <td> <%=trago.gradosalcohol%> </td>
        <td> <%=trago.fecha_caducidad%> </td>
        <td> <%=trago.precio%> </td>
        <td> <%=trago.tipo%> </td>
        <td>
            <form
                    action="/api/traguito/borrar"
                    method="post">
                <button>Eliminar</button>
            </form>
            <button>Editar </button>
        </td>
    </tr>
    <% })%>
</table>